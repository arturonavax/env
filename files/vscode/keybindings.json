// keybindings vscode
[
	{
		"key": "ctrl+alt+v",
		"command": "toggleVim"
	},
	{
		"key": "i",
		"command": "openEditors.newUntitledFile",
		"when": "activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "e",
		"command": "workbench.explorer.fileView.focus",
		"when": "activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "f",
		"command": "workbench.action.quickOpen",
		"when": "activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "w",
		"command": "workbench.action.findInFiles",
		"when": "activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "c",
		"command": "workbench.action.openApplicationSettingsJson",
		"when": "activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "k",
		"command": "workbench.action.openGlobalKeybindingsFile",
		"when": "activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "q",
		"command": "workbench.action.quit",
		"when": "activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "j",
		"command": "workbench.explorer.fileView.focus",
		"when": "activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	// Disable keys
	{
		"key": "ctrl+e",
		"command": "-actions.findWithSelection"
	},
	{
		"key": "ctrl+k ctrl+i",
		"command": "-editor.action.showHover",
		"when": "editorTextFocus"
	},
	{
		"key": "shift+tab",
		"command": "-acceptAlternativeSelectedSuggestion",
		"when": "suggestWidgetVisible && textInputFocus && textInputFocus"
	},
	{
		"key": "ctrl+f",
		"command": "-vscode-neovim.ctrl-f",
		"when": "editorTextFocus && neovim.ctrlKeysNormal && neovim.init && neovim.mode != 'insert'"
	},
	{
		"key": "ctrl+j",
		"command": "-editor.action.insertLineAfter",
		"when": "editorTextFocus && neovim.ctrlKeysInsert && !neovim.recording && neovim.mode == 'insert'"
	},
	{
		"key": "ctrl+p",
		"command": "-workbench.action.quickOpenNavigateNextInFilePicker",
		"when": "inFilesPicker || inQuickOpen"
	},
	{
		"key": "ctrl+w",
		"command": "-workbench.action.terminal.killEditor",
		"when": "terminalEditorFocus && terminalFocus && terminalHasBeenCreated && resourceScheme == 'vscode-terminal' || terminalEditorFocus && terminalFocus && terminalProcessSupported && resourceScheme == 'vscode-terminal'"
	},
	{
		"key": "ctrl+w",
		"command": "-workbench.action.closeGroup",
		"when": "activeEditorGroupEmpty && multipleEditorGroups"
	},
	{
		"key": "ctrl+w",
		"command": "-workbench.action.closeActiveEditor"
	},
	{
		"key": "ctrl+n",
		"command": "-workbench.action.files.newUntitledFile"
	},
	{
		"key": "ctrl+p",
		"command": "-workbench.action.quickOpen"
	},
	{
		"key": "shift+ctrl+e",
		"command": "-workbench.view.explorer"
	},
	// Explorer keys
	{
		"key": "enter",
		"command": "-renameFile",
		"when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
	},
	{
		"key": "enter",
		"command": "list.select",
		"when": "explorerViewletVisible && filesExplorerFocus"
	},
	{
		"key": "r",
		"command": "renameFile",
		"when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
	},
	{
		"key": "d",
		"command": "deleteFile",
		"when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
	},
	{
		"key": "x",
		"command": "list.collapse",
		"when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
	},
	{
		"key": "a",
		"command": "explorer.newFile",
		"when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
	},
	{
		"key": "v",
		"command": "explorer.openToSide",
		"when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
	},
	{
		"key": "space n",
		"command": "workbench.files.action.focusFilesExplorer",
		"when": "!explorerViewletVisible && !editorFocus && !inputFocus"
	},
	{
		"key": "space n",
		"command": "workbench.action.closeSidebar",
		"when": "explorerViewletVisible && !editorFocus && !inputFocus"
	},
	{
		"key": "space e",
		"command": "workbench.action.closeSidebar",
		"when": "explorerViewletVisible && !editorFocus && !inputFocus"
	},
	{
		"key": "q",
		"command": "workbench.action.closeSidebar",
		"when": "explorerViewletVisible && explorerViewletFocus && !editorFocus && !inputFocus && !textInputFocus && !renameInputVisible"
	},
	{
		"key": "space e",
		"command": "workbench.files.action.focusFilesExplorer",
		"when": "!explorerViewletVisible && !editorFocus && !inputFocus"
	},
	// Move explorer and problems/warnings/references
	{
		"key": "q",
		"command": "closeReferenceSearch",
		"when": "referenceSearchVisible && !inputFocus"
	},
	{
		"key": "j",
		"command": "list.focusDown",
		"when": "listFocus && !inputFocus && !problemFocus"
	},
	{
		"key": "k",
		"command": "list.focusUp",
		"when": "listFocus && !inputFocus && !problemFocus"
	},
	{
		"key": "tab",
		"command": "list.focusDown",
		"when": "listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "shift+tab",
		"command": "list.focusUp",
		"when": "listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "ctrl+n",
		"command": "list.focusDown",
		"when": "listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "ctrl+p",
		"command": "list.focusUp",
		"when": "listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "ctrl+j",
		"command": "list.focusDown",
		"when": "listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "ctrl+k",
		"command": "list.focusUp",
		"when": "listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "l",
		"command": "workbench.action.navigateEditorGroups",
		"when": "filesExplorerFocus && !inputFocus"
	},
	{
		"key": "ctrl+w",
		"command": "workbench.action.navigateEditorGroups",
		"when": "problemFocus || problemsFilterFocus"
	},
	// Move filePicker and quickOpen
	{
		"key": "j",
		"command": "workbench.action.quickOpenNavigateNext",
		"when": "inFilesPicker || inQuickOpen && !inputFocus"
	},
	{
		"key": "k",
		"command": "workbench.action.quickOpenNavigatePrevious",
		"when": "inFilesPicker || inQuickOpen && !inputFocus"
	},
	{
		"key": "tab",
		"command": "workbench.action.quickOpenNavigateNext",
		"when": "inFilesPicker || inQuickOpen"
	},
	{
		"key": "shift+tab",
		"command": "workbench.action.quickOpenNavigatePrevious",
		"when": "inFilesPicker || inQuickOpen"
	},
	{
		"key": "ctrl+n",
		"command": "workbench.action.quickOpenNavigateNext",
		"when": "inFilesPicker || inQuickOpen"
	},
	{
		"key": "ctrl+p",
		"command": "workbench.action.quickOpenNavigatePrevious",
		"when": "inFilesPicker || inQuickOpen"
	},
	{
		"key": "ctrl+j",
		"command": "workbench.action.quickOpenNavigateNext",
		"when": "inFilesPicker || inQuickOpen"
	},
	{
		"key": "ctrl+k",
		"command": "workbench.action.quickOpenNavigatePrevious",
		"when": "inFilesPicker || inQuickOpen"
	},
	// Move searchEditor
	{
		"key": "tab",
		"command": "search.action.focusNextSearchResult",
		"when": "hasSearchResult || inSearchEditor"
	},
	{
		"key": "shift+tab",
		"command": "search.action.focusPreviousSearchResult",
		"when": "hasSearchResult || inSearchEditor"
	},
	{
		"key": "ctrl+n",
		"command": "search.action.focusNextSearchResult",
		"when": "hasSearchResult || inSearchEditor"
	},
	{
		"key": "ctrl+p",
		"command": "search.action.focusPreviousSearchResult",
		"when": "hasSearchResult || inSearchEditor"
	},
	{
		"key": "ctrl+j",
		"command": "search.action.focusNextSearchResult",
		"when": "hasSearchResult || inSearchEditor"
	},
	{
		"key": "ctrl+k",
		"command": "search.action.focusPreviousSearchResult",
		"when": "hasSearchResult || inSearchEditor"
	},
	// Move suggestions/autocomplete
	{
		"key": "tab",
		"command": "selectNextSuggestion",
		"when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
	},
	{
		"key": "shift+tab",
		"command": "selectPrevSuggestion",
		"when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
	},
	{
		"key": "ctrl+n",
		"command": "selectNextSuggestion",
		"when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
	},
	{
		"key": "ctrl+p",
		"command": "selectPrevSuggestion",
		"when": "editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
	},
	// Show hover
	{
		"key": "shift+k",
		"command": "editor.action.showHover",
		"when": "editorTextFocus"
	},
	// General and macros
	{
		"key": "ctrl+h",
		"command": "workbench.action.moveEditorLeftInGroup",
		"when": "editorFocus"
	},
	{
		"key": "ctrl+l",
		"command": "workbench.action.moveEditorRightInGroup",
		"when": "editorFocus"
	},
	{
		"key": "ctrl+p",
		"command": "workbench.action.pinEditor",
		"when": "!activeEditorIsPinned"
	},
	{
		"key": "ctrl+p",
		"command": "workbench.action.unpinEditor",
		"when": "activeEditorIsPinned"
	},
	{
		"key": "escape",
		"command": "workbench.action.navigateEditorGroups",
		"when": "!editorFocus"
	},
	{
		"key": "escape",
		"command": "workbench.action.closeSidebar",
		"when": "sideBarFocus"
	},
	{
		"key": "escape",
		"command": "extension.multiCommand.execute",
		"args": {
			"sequence": [
				"search.action.clearSearchResults",
				"revealInExplorer",
				"workbench.action.closeSidebar"
			]
		},
		"when": "inSearchEditor || searchViewletFocus || hasSearchResult"
	},
	{
		"key": "escape",
		"command": "extension.multiCommand.execute",
		"args": {
			"sequence": [
				"problems.action.clearFilterText",
				"workbench.actions.view.toggleProblems"
			]
		},
		"when": "problemFocus || problemsFilterFocus"
	}
]
