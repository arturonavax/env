// Keybindings vscode
[
	{
		"key": "ctrl+alt+v",
		"command": "toggleVim"
	},

	// Editor Empty (Dashboard)
	{
		"key": "i",
		"command": "openEditors.newUntitledFile",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "e",
		"command": "workbench.explorer.fileView.focus",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "f",
		"command": "workbench.action.quickOpen",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "w",
		"command": "workbench.action.findInFiles",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "c",
		"command": "workbench.action.openApplicationSettingsJson",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "q",
		"command": "workbench.action.quit",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "ctrl+w",
		"command": "workbench.files.action.focusFilesExplorer",
		"when": "vim.active && activeEditorGroupEmpty && !sideBarFocus && !editorFocus && !inputFocus"
	},
	{
		"key": "j",
		"command": "workbench.files.action.focusFilesExplorer",
		"when": "vim.active && activeEditorGroupEmpty && !sideBarFocus && !editorFocus && !inputFocus"
	},
	{
		"key": "k",
		"command": "workbench.files.action.focusFilesExplorer",
		"when": "vim.active && activeEditorGroupEmpty && !sideBarFocus && !editorFocus && !inputFocus"
	},

	// General
	{
		"key": "q",
		"command": "closeReferenceSearch",
		"when": "vim.active && referenceSearchVisible && !inputFocus"
	},
	{
		"key": "q",
		"command": "closeMarkersNavigation",
		"when": "vim.active && !activeEditorGroupEmpty && (!inputFocus || editorFocus) && (vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},
	{
		"key": "l",
		"command": "workbench.action.navigateEditorGroups",
		"when": "vim.active && filesExplorerFocus && !inputFocus"
	},
	{
		"key": "ctrl+c",
		"command": "workbench.action.closeActiveEditor",
		"when": "vim.active && editorTextFocus && vim.mode === 'Normal'"
	},
	{
		"key": "ctrl+w",
		"command": "workbench.action.navigateEditorGroups",
		"when": "vim.active && problemFocus || problemsFilterFocus"
	},
	{
		"key": "escape",
		"command": "workbench.action.navigateEditorGroups",
		"when": "vim.active && !editorFocus"
	},
	{
		"key": "space f",
		"command": "workbench.action.quickOpen",
		"when": "vim.active && (!inputFocus || editorFocus) && (vim.mode === 'Normal' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress')"
	},
	{
		"key": "space j",
		"command": "workbench.action.findInFiles",
		"when": "vim.active && (!inputFocus || editorFocus) && (vim.mode === 'Normal' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress')"
	},

	//// Remaps
	{
		"key": "ctrl+p",
		"command": "-workbench.action.quickOpen",
		"when": "vim.active"
	},
	{
		"key": "ctrl+n",
		"command": "-workbench.action.files.newUntitledFile",
		"when": "vim.active"
	},

	// Hover
	{
		"key": "shift+k",
		"command": "editor.action.showHover",
		"when": "vim.active && editorTextFocus && (vim.mode === 'Normal' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress')"
	},
	{
		"key": "j",
		"command": "editor.action.scrollDownHover",
		"when": "vim.active && editorHoverFocused"
	},
	{
		"key": "k",
		"command": "editor.action.scrollUpHover",
		"when": "vim.active && editorHoverFocused"
	},
	{
		"key": "h",
		"command": "editor.action.scrollLeftHover",
		"when": "vim.active && editorHoverFocused"
	},
	{
		"key": "l",
		"command": "editor.action.scrollRightHover",
		"when": "vim.active && editorHoverFocused"
	},

	// Move down

	//// ctrl+j
	{
		"key": "ctrl+j",
		"command": "list.focusDown",
		"when": "vim.active && listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "ctrl+j",
		"command": "selectNextSuggestion",
		"when": "vim.active && suggestWidgetVisible || suggestWidgetMultipleSuggestions"
	},
	{
		"key": "ctrl+j",
		"command": "workbench.action.quickOpenSelectNext",
		"when": "vim.active && inQuickOpen || inFilesPicker"
	},
	{
		"key": "ctrl+j",
		"command": "search.action.focusNextSearchResult",
		"when": "vim.active && hasSearchResult || inSearchEditor"
	},
	{
		"key": "ctrl+j",
		"command": "selectNextCodeAction",
		"when": "vim.active && codeActionMenuVisible"
	},

	//// j
	{
		"key": "j",
		"command": "list.focusDown",
		"when": "vim.active && listFocus && !inputFocus && !problemFocus"
	},

	//// tab
	{
		"key": "tab",
		"command": "list.focusDown",
		"when": "vim.active && listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "tab",
		"command": "workbench.action.quickOpenSelectNext",
		"when": "vim.active && inFilesPicker || inQuickOpen"
	},
	{
		"key": "tab",
		"command": "selectNextSuggestion",
		"when": "vim.active && editorTextFocus && (suggestWidgetMultipleSuggestions || suggestWidgetVisible)"
	},

	//// ctrl+n
	{
		"key": "ctrl+n",
		"command": "list.focusDown",
		"when": "vim.active && listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "ctrl+n",
		"command": "workbench.action.quickOpenSelectNext",
		"when": "vim.active && inFilesPicker || inQuickOpen"
	},
	{
		"key": "ctrl+n",
		"command": "selectNextSuggestion",
		"when": "vim.active && editorTextFocus && (suggestWidgetMultipleSuggestions || suggestWidgetVisible"
	},
	{
		"key": "ctrl+n",
		"command": "selectNextCodeAction",
		"when": "vim.active && codeActionMenuVisible"
	},

	// Move up

	//// ctrl+k
	{
		"key": "ctrl+k",
		"command": "list.focusUp",
		"when": "vim.active && listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "ctrl+k",
		"command": "selectPrevSuggestion",
		"when": "vim.active && suggestWidgetVisible || suggestWidgetMultipleSuggestions"
	},
	{
		"key": "ctrl+k",
		"command": "workbench.action.quickOpenSelectPrevious",
		"when": "vim.active && inQuickOpen || inFilesPicker"
	},
	{
		"key": "ctrl+k",
		"command": "search.action.focusPreviousSearchResult",
		"when": "vim.active && hasSearchResult || inSearchEditor"
	},
	{
		"key": "ctrl+k",
		"command": "selectPrevCodeAction",
		"when": "vim.active && codeActionMenuVisible"
	},

	//// k
	{
		"key": "k",
		"command": "list.focusUp",
		"when": "vim.active && listFocus && !inputFocus && !problemFocus"
	},

	//// shift+tab
	{
		"key": "shift+tab",
		"command": "list.focusUp",
		"when": "vim.active && listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "shift+tab",
		"command": "workbench.action.quickOpenSelectPrevious",
		"when": "vim.active && inFilesPicker || inQuickOpen"
	},
	{
		"key": "shift+tab",
		"command": "selectPrevSuggestion",
		"when": "vim.active && editorTextFocus && (suggestWidgetMultipleSuggestions || suggestWidgetVisible)"
	},

	//// ctrl+p
	{
		"key": "ctrl+p",
		"command": "list.focusUp",
		"when": "vim.active && listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "ctrl+p",
		"command": "workbench.action.quickOpenSelectPrevious",
		"when": "vim.active && inFilesPicker || inQuickOpen"
	},
	{
		"key": "ctrl+p",
		"command": "selectPrevSuggestion",
		"when": "vim.active && editorTextFocus && (suggestWidgetMultipleSuggestions || suggestWidgetVisible)"
	},
	{
		"key": "ctrl+p",
		"command": "selectPrevCodeAction",
		"when": "vim.active && codeActionMenuVisible"
	},

	// File explorer

	//// Open and close
	{
		"key": "q",
		"command": "workbench.action.closeSidebar",
		"when": "vim.active && explorerViewletVisible && explorerViewletFocus && !editorFocus && !inputFocus && !textInputFocus && !renameInputVisible"
	},
	{
		"key": "space n",
		"command": "workbench.files.action.focusFilesExplorer",
		"when": "vim.active && !explorerViewletVisible && (!inputFocus || editorFocus) && (vim.mode === 'Normal' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress')"
	},
	{
		"key": "space n",
		"command": "workbench.action.closeSidebar",
		"when": "vim.active && explorerViewletVisible && (!inputFocus || editorFocus) && (vim.mode === 'Normal' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress')"
	},
	{
		"key": "space e",
		"command": "revealInExplorer",
		"when": "vim.active && !explorerViewletVisible && (!inputFocus || editorFocus) && (vim.mode === 'Normal' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress')"
	},
	{
		"key": "space e",
		"command": "workbench.action.closeSidebar",
		"when": "vim.active && explorerViewletVisible && (!inputFocus || editorFocus) && (vim.mode === 'Normal' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress')"
	},
	{
		"key": "space t",
		"command": "workbench.action.toggleSidebarVisibility",
		"when": "vim.active && (!inputFocus || editorFocus) && (vim.mode === 'Normal' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress')"
	},
	{
		"key": "space t",
		"command": "workbench.action.toggleSidebarVisibility",
		"when": "vim.active && (!inputFocus || editorFocus) && (vim.mode === 'Normal' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress')"
	},

	//// Actions
	{
		"key": "ctrl+h",
		"command": "list.collapse",
		"when": "vim.active && listFocus && !inputFocus"
	},
	{
		"key": "ctrl+l",
		"command": "list.expand",
		"when": "vim.active && listFocus && !inputFocus"
	},
	{
		"key": "enter",
		"command": "list.select",
		"when": "vim.active && explorerViewletVisible && filesExplorerFocus"
	},
	{
		"key": "v",
		"command": "explorer.openToSide",
		"when": "vim.active && explorerViewletVisible && filesExplorerFocus && !inputFocus"
	},
	{
		"key": "r",
		"command": "renameFile",
		"when": "vim.active && explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
	},
	{
		"key": "d",
		"command": "deleteFile",
		"when": "vim.active && explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
	},
	{
		"key": "x",
		"command": "list.collapse",
		"when": "vim.active && explorerViewletVisible && filesExplorerFocus && !inputFocus"
	},
	{
		"key": "a",
		"command": "explorer.newFile",
		"when": "vim.active && explorerViewletVisible && filesExplorerFocus && !inputFocus"
	},
	{
		"key": "y",
		"command": "filesExplorer.copy",
		"when": "vim.active && explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !inputFocus"
	},
	{
		"key": "/",
		"command": "workbench.action.quickOpen",
		"when": "vim.active && explorerViewletVisible && filesExplorerFocus && (!inputFocus || editorFocus) && (vim.mode === 'Normal' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress')"
	},
	{
		"key": "ctrl+w",
		"command": "workbench.action.focusRightGroup",
		"when": "vim.active && explorerViewletVisible && !editorFocus && !inputFocus"
	},

	//// Remaps
	{
		"key": "enter",
		"command": "-renameFile",
		"when": "vim.active && explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
	},

	// Tab control
	{
		"key": "shift+h",
		"command": "workbench.action.previousEditorInGroup",
		"when": "vim.active && vim.mode !== 'Insert'"
	},
	{
		"key": "shift+l",
		"command": "workbench.action.nextEditorInGroup",
		"when": "vim.active && vim.mode !== 'Insert'"
	},
	{
		"key": "ctrl+h",
		"command": "workbench.action.moveEditorLeftInGroup",
		"when": "vim.active && editorFocus"
	},
	{
		"key": "ctrl+l",
		"command": "workbench.action.moveEditorRightInGroup",
		"when": "vim.active && editorFocus"
	},
	{
		"key": "ctrl+p",
		"command": "workbench.action.pinEditor",
		"when": "vim.active && !activeEditorIsPinned && vim.mode === 'Normal'"
	},
	{
		"key": "ctrl+p",
		"command": "workbench.action.unpinEditor",
		"when": "vim.active && activeEditorIsPinned && vim.mode === 'Normal'"
	},

	// Clear exits
	{
		"key": "escape",
		"command": "runCommands",
		"args": {
			"commands": [
				"search.action.clearSearchResults",
				"revealInExplorer",
				"workbench.action.closeSidebar"
			]
		},
		"when": "vim.active && inSearchEditor || searchViewletFocus || hasSearchResult"
	},
	{
		"key": "escape",
		"command": "runCommands",
		"args": {
			"commands": [
				"problems.action.clearFilterText",
				"workbench.actions.view.toggleProblems"
			]
		},
		"when": "vim.active && problemFocus || problemsFilterFocus"
	},

	// Actions
	{
		"key": "space l",
		"command": "workbench.action.showErrorsWarnings",
		"when": "vim.active && editorFocus && (!inputFocus || editorFocus) && (vim.mode === 'Normal' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress')"
	},
	{
		"key": "space a",
		"command": "editor.action.codeAction",
		"when": "vim.active && editorHasCodeActionsProvider && (!inputFocus || editorFocus) && (vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress')"
	},
	{
		"key": "space s",
		"command": "codelens.showLensesInCurrentLine",
		"when": "vim.active && (!inputFocus || editorFocus) && (vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress')"
	},
	{
		"key": "space r n",
		"command": "editor.action.rename",
		"when": "vim.active && editorFocus && (!inputFocus || editorFocus) && (vim.mode === 'Normal' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress')"
	},
	{
		"key": "space r r",
		"command": "editor.action.refactor",
		"when": "vim.active && (!inputFocus || editorFocus) && editorHasCodeActionsProvider && (vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress')"
	},

	//// Language tools
	{
		"key": "space d",
		"command": "go.show.commands",
		"when": "vim.active && (!inputFocus || editorFocus) && (vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress') && editorLangId === 'go'"
	},

	// Folding
	{
		"key": "z c",
		"command": "editor.fold",
		"when": "vim.active && (!inputFocus || editorFocus) && (vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress') && editorTextFocus && foldingEnabled"
	},
	{
		"key": "z o",
		"command": "editor.unfold",
		"when": "vim.active && (!inputFocus || editorFocus) && (vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress') && editorTextFocus && foldingEnabled"
	},
	{
		"key": "z shift+r",
		"command": "editor.unfoldAll",
		"when": "vim.active && (!inputFocus || editorFocus) && (vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress') && editorTextFocus && foldingEnabled"
	},
	{
		"key": "z shift+m",
		"command": "editor.foldAll",
		"when": "vim.active && (!inputFocus || editorFocus) && (vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress') && editorTextFocus && foldingEnabled"
	},
	{
		"key": "z shift+c",
		"command": "editor.unfoldRecursively",
		"when": "vim.active && (!inputFocus || editorFocus) && (vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress') && editorTextFocus && foldingEnabled"
	},
	{
		"key": "z shift+O",
		"command": "editor.foldRecursively",
		"when": "vim.active && (!inputFocus || editorFocus) && (vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress') && editorTextFocus && foldingEnabled"
	}
]
