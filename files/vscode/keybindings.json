// Keybindings vscode
[
	{
		"key": "ctrl+alt+v",
		"command": "toggleVim"
	},
	{
		"key": "ctrl+h",
		"command": "editor.action.triggerSuggest",
		"when": "vim.active && vim.mode === 'Insert' && editorHasCompletionItemProvider && editorTextFocus && !editorReadonly"
	},
	{
		"key": "ctrl+h",
		"command": "toggleSuggestionDetails",
		"when": "vim.active && vim.mode === 'Insert' && editorTextFocus && suggestWidgetVisible"
	},
	{
		"key": "ctrl+alt+h",
		"command": "toggleSuggestionFocus",
		"when": "vim.active && vim.mode === 'Insert' && editorTextFocus && suggestWidgetVisible"
	},
	{
		"key": "ctrl+shift+e",
		"command": "runCommands",
		"args": {
			"commands": [
				"workbench.view.explorer",
				"workbench.action.focusActiveEditorGroup"
			]
		}
	},
	{
		"key": "ctrl+shift+f",
		"command": "runCommands",
		"args": {
			"commands": [
				"workbench.action.findInFiles",
				"workbench.action.focusActiveEditorGroup"
			]
		}
	},
	{
		"key": "ctrl+shift+x",
		"command": "runCommands",
		"args": {
			"commands": [
				"workbench.view.extensions",
				"workbench.action.focusActiveEditorGroup"
			]
		}
	},
	{
		"key": "ctrl+shift+t",
		"command": "runCommands",
		"args": {
			"commands": [
				"workbench.view.extension.test",
				"workbench.action.focusActiveEditorGroup"
			]
		}
	},
	{
		"key": "ctrl+shift+y",
		"command": "runCommands",
		"args": {
			"commands": [
				"todo-tree-view.focus",
				"workbench.action.focusActiveEditorGroup"
			]
		}
	},
	{
		"key": "ctrl+shift+d",
		"command": "runCommands",
		"args": {
			"commands": ["debug.openView", "workbench.action.focusActiveEditorGroup"]
		}
	},
	{
		"key": "ctrl+shift+g",
		"command": "runCommands",
		"args": {
			"commands": [
				"workbench.view.scm",
				"workbench.action.focusActiveEditorGroup"
			]
		}
	},
	{
		"key": "ctrl+shift+c",
		"command": "workbench.panel.chat.view.copilot.focus"
	},
	{
		"key": "ctrl+shift+p",
		"command": "projectManager.listProjects"
	},
	{
		"key": "ctrl+shift+cmd+p",
		"command": "projectManager.listProjectsNewWindow"
	},
	{
		"key": "ctrl+w",
		"command": "-workbench.action.switchWindow"
	},
	{
		"key": "alt+w",
		"command": "workbench.action.switchWindow"
	},
	{
		"key": "ctrl+shift+o",
		"command": "workbench.action.files.openFolder"
	},

	// Editor Empty (Dashboard)
	{
		"key": "i",
		"command": "openEditors.newUntitledFile",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "e",
		"command": "workbench.explorer.fileView.focus",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "f",
		"command": "workbench.action.quickOpen",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "w",
		"command": "workbench.action.findInFiles",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "c",
		"command": "workbench.action.openApplicationSettingsJson",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "q",
		"command": "workbench.action.closeWindow",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "j",
		"command": "workbench.files.action.focusFilesExplorer",
		"when": "vim.active && activeEditorGroupEmpty && !sideBarFocus && !editorFocus && !inputFocus"
	},
	{
		"key": "k",
		"command": "workbench.files.action.focusFilesExplorer",
		"when": "vim.active && activeEditorGroupEmpty && !sideBarFocus && !editorFocus && !inputFocus"
	},
	{
		"key": "p",
		"command": "projectManager.listProjects",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "u",
		"command": "workbench.action.openRecent",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "o",
		"command": "workbench.action.files.openFolder",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "x",
		"command": "revealFileInOS",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},

	// General
	{
		"key": "q",
		"command": "closeReferenceSearch",
		"when": "vim.active && referenceSearchVisible || inReferenceSearchEditor && (!inputFocus || editorFocus) && (vim.mode === 'Normal' && vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},
	{
		"key": "q",
		"command": "closeMarkersNavigation",
		"when": "vim.active && !activeEditorGroupEmpty && markersNavigationVisible && (!inputFocus || editorFocus) && (vim.mode === 'Normal' && vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},
	{
		"key": "q",
		"command": "workbench.action.togglePanel",
		"when": "vim.active && (panelFocus && activePanel === 'workbench.view.extension.tooltitude-refs') && !referenceSearchVisible && !inReferenceSearchEditor"
	},
	{
		"key": "q",
		"command": "editor.closeTestPeek",
		"when": "vim.active && !activeEditorGroupEmpty && testing.isPeekVisible && (!inputFocus || editorFocus) && (vim.mode === 'Normal' && vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},
	{
		"key": "l",
		"command": "workbench.action.navigateEditorGroups",
		"when": "vim.active && filesExplorerFocus && !inputFocus"
	},
	{
		"key": "ctrl+c",
		"command": "workbench.action.closeActiveEditor",
		"when": "vim.active && editorTextFocus && (vim.mode === 'Normal' && vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},

	{
		"key": "space f",
		"command": "workbench.action.quickOpen",
		"when": "vim.active && (!inputFocus || editorFocus) && (vim.mode === 'Normal' && vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},
	{
		"key": "space j",
		"command": "workbench.action.findInFiles",
		// "command": "workbench.action.experimental.quickTextSearch",
		"when": "vim.active && (!inputFocus || editorFocus) && (vim.mode === 'Normal' && vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},
	{
		"key": "v",
		"command": "explorer.openToSide",
		"when": "vim.active && filesExplorerFocus && !textInputFocus && !inputFocus && !editorFocus"
	},
	{
		"key": "v",
		"command": "openReferenceToSide",
		"when": "vim.active && referenceSearchVisible || inReferenceSearchEditor && !textInputFocus && !inputFocus && (vim.mode === 'Normal' && vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},
	{
		"key": "ctrl+v",
		"command": "workbench.action.alternativeAcceptSelectedQuickOpenItem",
		"when": "vim.active && inFilesPicker && (vim.mode === 'Normal' && vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},
	{
		"key": "ctrl+v",
		"command": "problems.action.openToSide",
		"when": "vim.active && problemFocus || problemsFilterFocus && (vim.mode === 'Normal' && vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},
	{
		"key": "ctrl+c",
		"command": "workbench.action.closeQuickOpen",
		"when": "vim.active && inQuickOpen || inFilesPicker"
	},
	{
		"key": "h",
		"command": "list.collapse",
		"when": "vim.active && listFocus && !inputFocus"
	},
	{
		"key": "l",
		"command": "list.expand",
		"when": "vim.active && listFocus && !inputFocus"
	},
	{
		"key": "enter",
		"command": "list.select",
		"when": "vim.active && listFocus && !inputFocus"
	},
	{
		"key": "ctrl+w j",
		"command": "widgetNavigation.focusNext",
		"when": "vim.active && !editorFocus && ((vim.mode === 'Normal' || vim.mode === 'Visual' || vim.mode === 'VisualLine' || vim.mode === 'VisualBlock') && vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},
	{
		"key": "ctrl+w k",
		"command": "widgetNavigation.focusPrevious",
		"when": "vim.active && !editorFocus && ((vim.mode === 'Normal' || vim.mode === 'Visual' || vim.mode === 'VisualLine' || vim.mode === 'VisualBlock') && vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},
	{
		"key": "ctrl+w shift+=",
		"command": "workbench.action.increaseViewHeight",
		"when": "vim.active && (focusedView === 'workbench.panel.output' || focusedView === 'terminal' || focusedView === 'workbench.panel.markers.view' || focusedView === 'workbench.panel.testResults.view' || focusedView === 'workbench.panel.repl.view' || focusedView === '~remote.forwardedPorts' || (panelFocus && activePanel === 'workbench.view.extension.tooltitude-refs') || problemsFilterFocus) && (vim.mode === 'Normal' && vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},
	{
		"key": "ctrl+w -",
		"command": "workbench.action.decreaseViewHeight",
		"when": "vim.active && (focusedView === 'workbench.panel.output' || focusedView === 'terminal' || focusedView === 'workbench.panel.markers.view' || focusedView === 'workbench.panel.testResults.view' || focusedView === 'workbench.panel.repl.view' || focusedView === '~remote.forwardedPorts' || (panelFocus && activePanel === 'workbench.view.extension.tooltitude-refs') || problemsFilterFocus) && (vim.mode === 'Normal' && vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},
	{
		"key": "ctrl+w =",
		"command": "workbench.action.evenEditorWidths",
		"when": "vim.active && (focusedView === 'workbench.panel.output' || focusedView === 'terminal' || focusedView === 'workbench.panel.markers.view' || focusedView === 'workbench.panel.testResults.view' || focusedView === 'workbench.panel.repl.view' || focusedView === '~remote.forwardedPorts' || (panelFocus && activePanel === 'workbench.view.extension.tooltitude-refs') || problemsFilterFocus) && (vim.mode === 'Normal' && vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},
	{
		"key": "ctrl+z",
		"command": "workbench.action.toggleMaximizedPanel",
		"when": "vim.active && !editorFocus && (focusedView === 'workbench.panel.output' || focusedView === 'terminal' || focusedView === 'workbench.panel.markers.view' || focusedView === 'workbench.panel.testResults.view' || focusedView === 'workbench.panel.repl.view' || focusedView === '~remote.forwardedPorts' || (panelFocus && activePanel === 'workbench.view.extension.tooltitude-refs') || problemsFilterFocus)"
	},
	{
		"key": "ctrl+z",
		"command": "workbench.action.toggleMaximizeEditorGroup",
		"when": "vim.active && editorFocus && (vim.mode === 'Normal' && vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},

	//// Remaps
	{
		"key": "ctrl+p",
		"command": "-workbench.action.quickOpen",
		"when": "vim.active"
	},
	{
		"key": "ctrl+n",
		"command": "-workbench.action.files.newUntitledFile"
	},
	{
		"key": "ctrl+w",
		"command": "-workbench.action.closeActiveEditor"
	},
	{
		"key": "ctrl+shift+v",
		"command": "-markdown.showPreview",
		"when": "!notebookEditorFocused && editorLangId == 'markdown'"
	},
	{
		"key": "cmd+m",
		"command": "-markdown.extension.editing.toggleMath",
		"when": "editorTextFocus && !editorReadonly && editorLangId =~ /^markdown$|^rmd$|^quarto$/"
	},

	// Hover
	{
		"key": "shift+k",
		"command": "editor.action.showHover",
		"when": "vim.active && editorTextFocus && ((vim.mode === 'Normal' || vim.mode === 'Visual' || vim.mode === 'VisualLine' || vim.mode === 'VisualBlock') && vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},
	{
		"key": "j",
		"command": "editor.action.scrollDownHover",
		"when": "vim.active && editorHoverFocused"
	},
	{
		"key": "k",
		"command": "editor.action.scrollUpHover",
		"when": "vim.active && editorHoverFocused"
	},
	{
		"key": "h",
		"command": "editor.action.scrollLeftHover",
		"when": "vim.active && editorHoverFocused"
	},
	{
		"key": "l",
		"command": "editor.action.scrollRightHover",
		"when": "vim.active && editorHoverFocused"
	},
	{
		"key": "g g",
		"command": "editor.action.goToTopHover",
		"when": "vim.active && editorHoverFocused"
	},
	{
		"key": "shift+g",
		"command": "editor.action.goToBottomHover",
		"when": "vim.active && editorHoverFocused"
	},
	{
		"key": "ctrl+d",
		"command": "editor.action.pageDownHover",
		"when": "vim.active && editorHoverFocused"
	},
	{
		"key": "ctrl+u",
		"command": "editor.action.pageUpHover",
		"when": "vim.active && editorHoverFocused"
	},
	{
		"key": "q",
		"command": "workbench.action.focusLastEditorGroup",
		"when": "vim.active && editorHoverFocused"
	},

	// Panels
	{
		"key": "ctrl+c",
		"command": "workbench.action.togglePanel",
		"when": "vim.active && (focusedView === 'workbench.panel.output' || focusedView === 'workbench.panel.testResults.view') && (vim.mode === 'Normal' && vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},
	{
		"key": "q",
		"command": "workbench.action.closePanel",
		"when": "vim.active && (focusedView === 'workbench.panel.output' || focusedView === 'workbench.panel.testResults.view') && (vim.mode === 'Normal' && vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},
	{
		"key": "ctrl+w k",
		"command": "workbench.action.focusActiveEditorGroup",
		"when": "vim.active && (focusedView === 'workbench.panel.output' || focusedView === 'terminal' || focusedView === 'workbench.panel.markers.view' || focusedView === 'workbench.panel.testResults.view' || focusedView === 'workbench.panel.repl.view' || focusedView === '~remote.forwardedPorts' || (panelFocus && activePanel === 'workbench.view.extension.tooltitude-refs') || problemsFilterFocus) && (vim.mode === 'Normal' && vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},
	{
		"key": "ctrl+w h",
		"command": "workbench.action.focusSideBar",
		"when": "vim.active && (focusedView === 'workbench.panel.output' || focusedView === 'terminal' || focusedView === 'workbench.panel.markers.view' || focusedView === 'workbench.panel.testResults.view' || focusedView === 'workbench.panel.repl.view' || focusedView === '~remote.forwardedPorts' || (panelFocus && activePanel === 'workbench.view.extension.tooltitude-refs') || problemsFilterFocus) && (vim.mode === 'Normal' && vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},
	{
		"key": "space p",
		"command": "workbench.action.togglePanel",
		"when": "vim.active && !terminal && (!inputFocus || editorFocus) && (vim.mode === 'Normal' && vim.mode !== 'Insert' && vim.mode !== 'SearchInProgressMode' && vim.mode !== 'CommandlineInProgress' && vim.mode !== 'EasyMotionMode' && vim.mode !== 'EasyMotionInputMode' && vim.mode !== 'SurroundInputMode')"
	},

	// Move down

	//// ctrl+j
	{
		"key": "ctrl+j",
		"command": "list.focusDown",
		"when": "vim.active && listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "ctrl+j",
		"command": "selectNextSuggestion",
		"when": "vim.active && suggestWidgetVisible || suggestWidgetMultipleSuggestions"
	},
	{
		"key": "ctrl+j",
		"command": "workbench.action.quickOpenSelectNext",
		"when": "vim.active && inQuickOpen || inFilesPicker"
	},
	{
		"key": "ctrl+j",
		"command": "search.action.focusNextSearchResult",
		"when": "vim.active && (hasSearchResult || inSearchEditor) && activeViewlet === 'workbench.view.search' && !referenceSearchVisible && !inQuickOpen && !inFilesPicker && !problemsFilterFocus && !problemFocus"
	},
	{
		"key": "ctrl+j",
		"command": "selectNextCodeAction",
		"when": "vim.active && codeActionMenuVisible"
	},

	//// j
	{
		"key": "j",
		"command": "list.focusDown",
		"when": "vim.active && listFocus && !inputFocus && !problemFocus"
	},

	//// tab
	{
		"key": "tab",
		"command": "list.focusDown",
		"when": "vim.active && listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "tab",
		"command": "workbench.action.quickOpenSelectNext",
		"when": "vim.active && inFilesPicker || inQuickOpen"
	},
	{
		"key": "tab",
		"command": "selectNextSuggestion",
		"when": "vim.active && suggestWidgetMultipleSuggestions || suggestWidgetVisible"
	},

	//// ctrl+n
	{
		"key": "ctrl+n",
		"command": "list.focusDown",
		"when": "vim.active && listFocus || problemFocus || problemsFilterFocus || inSearchEditor"
	},
	{
		"key": "ctrl+n",
		"command": "workbench.action.quickOpenSelectNext",
		"when": "vim.active && inFilesPicker || inQuickOpen"
	},
	{
		"key": "ctrl+n",
		"command": "search.action.focusNextSearchResult",
		"when": "vim.active && (hasSearchResult || inSearchEditor) && activeViewlet === 'workbench.view.search' && !referenceSearchVisible && !inQuickOpen && !inFilesPicker && !problemsFilterFocus && !problemFocus"
	},
	{
		"key": "ctrl+n",
		"command": "selectNextSuggestion",
		"when": "vim.active && suggestWidgetMultipleSuggestions || suggestWidgetVisible"
	},
	{
		"key": "ctrl+n",
		"command": "selectNextCodeAction",
		"when": "vim.active && codeActionMenuVisible"
	},

	// Move up

	//// ctrl+k
	{
		"key": "ctrl+k",
		"command": "list.focusUp",
		"when": "vim.active && listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "ctrl+k",
		"command": "selectPrevSuggestion",
		"when": "vim.active && suggestWidgetVisible || suggestWidgetMultipleSuggestions"
	},
	{
		"key": "ctrl+k",
		"command": "workbench.action.quickOpenSelectPrevious",
		"when": "vim.active && inQuickOpen || inFilesPicker"
	},
	{
		"key": "ctrl+k",
		"command": "search.action.focusPreviousSearchResult",
		"when": "vim.active && (hasSearchResult || inSearchEditor) && activeViewlet === 'workbench.view.search' && !referenceSearchVisible && !inQuickOpen && !inFilesPicker && !problemsFilterFocus && !problemFocus"
	},
	{
		"key": "ctrl+k",
		"command": "selectPrevCodeAction",
		"when": "vim.active && codeActionMenuVisible"
	},

	//// k
	{
		"key": "k",
		"command": "list.focusUp",
		"when": "vim.active && listFocus && !inputFocus && !problemFocus"
	},

	//// shift+tab
	{
		"key": "shift+tab",
		"command": "list.focusUp",
		"when": "vim.active && listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "shift+tab",
		"command": "workbench.action.quickOpenSelectPrevious",
		"when": "vim.active && inFilesPicker || inQuickOpen"
	},
	{
		"key": "shift+tab",
		"command": "selectPrevSuggestion",
		"when": "vim.active && suggestWidgetMultipleSuggestions || suggestWidgetVisible"
	},

	//// ctrl+p
	{
		"key": "ctrl+p",
		"command": "list.focusUp",
		"when": "vim.active && listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "ctrl+p",
		"command": "workbench.action.quickOpenSelectPrevious",
		"when": "vim.active && inFilesPicker || inQuickOpen"
	},
	{
		"key": "ctrl+p",
		"command": "search.action.focusPreviousSearchResult",
