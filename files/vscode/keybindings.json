// keybindings vscode
[
	// General
	{
		"key": "ctrl+alt+v",
		"command": "toggleVim"
	},
	{
		"key": "i",
		"command": "openEditors.newUntitledFile",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "e",
		"command": "workbench.explorer.fileView.focus",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "f",
		"command": "workbench.action.quickOpen",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "w",
		"command": "workbench.action.findInFiles",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "c",
		"command": "workbench.action.openApplicationSettingsJson",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "q",
		"command": "workbench.action.quit",
		"when": "vim.active && activeEditorGroupEmpty && !editorFocus && !inputFocus"
	},
	{
		"key": "ctrl+w",
		"command": "workbench.files.action.focusFilesExplorer",
		"when": "vim.active && activeEditorGroupEmpty && !sideBarFocus && !editorFocus && !inputFocus"
	},
	{
		"key": "ctrl+c",
		"command": "workbench.action.closeActiveEditor",
		"when": "vim.active && editorFocus && !textInputFocus && !inputFocus"
	},
	{
		"key": "j",
		"command": "workbench.files.action.focusFilesExplorer",
		"when": "vim.active && activeEditorGroupEmpty && !sideBarFocus && !editorFocus && !inputFocus"
	},
	{
		"key": "k",
		"command": "workbench.files.action.focusFilesExplorer",
		"when": "vim.active && activeEditorGroupEmpty && !sideBarFocus && !editorFocus && !inputFocus"
	},
	{
		"key": "h",
		"command": "editor.action.scrollLeftHover",
		"when": "editorHoverFocused"
	},
	{
		"key": "j",
		"command": "editor.action.scrollDownHover",
		"when": "editorHoverFocused"
	},
	{
		"key": "k",
		"command": "editor.action.scrollUpHover",
		"when": "editorHoverFocused"
	},
	{
		"key": "l",
		"command": "editor.action.scrollRightHover",
		"when": "editorHoverFocused"
	},
	{
		"key": "shift+k",
		"command": "editor.action.showHover",
		"when": "vim.active && editorTextFocus"
	},
	{
		"key": "space r n",
		"command": "editor.action.rename",
		"when": "vim.active && editorFocus && (!inputFocus || editorFocus) && vim.mode == 'Normal'"
	},
	{
		"key": "space l",
		"command": "workbench.action.showErrorsWarnings",
		"when": "vim.active && editorFocus && (!inputFocus || editorFocus) && vim.mode == 'Normal'"
	},
	{
		"key": "ctrl+h",
		"command": "list.collapse",
		"when": "vim.active && listFocus && !inputFocus"
	},
	{
		"key": "ctrl+l",
		"command": "list.expand",
		"when": "vim.active && listFocus && !inputFocus"
	},
	{
		"key": "ctrl+k",
		"command": "list.focusUp",
		"when": "vim.active && listFocus && !inputFocus"
	},
	{
		"key": "ctrl+j",
		"command": "list.focusDown",
		"when": "vim.active && listFocus && !inputFocus"
	},
	{
		"key": "ctrl+j",
		"command": "selectNextSuggestion",
		"when": "vim.active && suggestWidgetVisible"
	},
	{
		"key": "ctrl+k",
		"command": "selectPrevSuggestion",
		"when": "vim.active && suggestWidgetVisible"
	},
	{
		"key": "ctrl+j",
		"command": "workbench.action.quickOpenSelectNext",
		"when": "vim.active && inQuickOpen"
	},
	{
		"key": "ctrl+k",
		"command": "workbench.action.quickOpenSelectPrevious",
		"when": "vim.active && inQuickOpen"
	},
	// Move explorer and problems/warnings/references
	{
		"key": "q",
		"command": "closeReferenceSearch",
		"when": "vim.active && referenceSearchVisible && !inputFocus"
	},
	{
		"key": "j",
		"command": "list.focusDown",
		"when": "vim.active && listFocus && !inputFocus && !problemFocus"
	},
	{
		"key": "k",
		"command": "list.focusUp",
		"when": "vim.active && listFocus && !inputFocus && !problemFocus"
	},
	{
		"key": "tab",
		"command": "list.focusDown",
		"when": "vim.active && listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "shift+tab",
		"command": "list.focusUp",
		"when": "vim.active && listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "ctrl+n",
		"command": "list.focusDown",
		"when": "vim.active && listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "ctrl+p",
		"command": "list.focusUp",
		"when": "vim.active && listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "ctrl+j",
		"command": "list.focusDown",
		"when": "vim.active && listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "ctrl+k",
		"command": "list.focusUp",
		"when": "vim.active && listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "l",
		"command": "workbench.action.navigateEditorGroups",
		"when": "vim.active && filesExplorerFocus && !inputFocus"
	},
	{
		"key": "ctrl+w",
		"command": "workbench.action.navigateEditorGroups",
		"when": "vim.active && problemFocus || problemsFilterFocus"
	},
	// Move filePicker and quickOpen
	{
		"key": "j",
		"command": "workbench.action.quickOpenNavigateNext",
		"when": "vim.active && inFilesPicker || inQuickOpen && !inputFocus"
	},
	{
		"key": "k",
		"command": "workbench.action.quickOpenNavigatePrevious",
		"when": "vim.active && inFilesPicker || inQuickOpen && !inputFocus"
	},
	{
		"key": "tab",
		"command": "workbench.action.quickOpenNavigateNext",
		"when": "vim.active && inFilesPicker || inQuickOpen"
	},
	{
		"key": "shift+tab",
		"command": "workbench.action.quickOpenNavigatePrevious",
		"when": "vim.active && inFilesPicker || inQuickOpen"
	},
	{
		"key": "ctrl+n",
		"command": "workbench.action.quickOpenNavigateNext",
		"when": "vim.active && inFilesPicker || inQuickOpen"
	},
	{
		"key": "ctrl+p",
		"command": "workbench.action.quickOpenNavigatePrevious",
		"when": "vim.active && inFilesPicker || inQuickOpen"
	},
	{
		"key": "ctrl+j",
		"command": "workbench.action.quickOpenNavigateNext",
		"when": "vim.active && inFilesPicker || inQuickOpen"
	},
	{
		"key": "ctrl+k",
		"command": "workbench.action.quickOpenNavigatePrevious",
		"when": "vim.active && inFilesPicker || inQuickOpen"
	},
	// Move searchEditor
	{
		"key": "ctrl+j",
		"command": "search.action.focusNextSearchResult",
		"when": "vim.active && hasSearchResult || inSearchEditor"
	},
	{
		"key": "ctrl+k",
		"command": "search.action.focusPreviousSearchResult",
		"when": "vim.active && hasSearchResult || inSearchEditor"
	},
	// Move suggestions/autocomplete
	{
		"key": "tab",
		"command": "selectNextSuggestion",
		"when": "vim.active && editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
	},
	{
		"key": "shift+tab",
		"command": "selectPrevSuggestion",
		"when": "vim.active && editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
	},
	{
		"key": "ctrl+n",
		"command": "selectNextSuggestion",
		"when": "vim.active && editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
	},
	{
		"key": "ctrl+p",
		"command": "selectPrevSuggestion",
		"when": "vim.active && editorTextFocus && suggestWidgetMultipleSuggestions && suggestWidgetVisible"
	},
	// Explorer keys
	{
		"key": "enter",
		"command": "-renameFile",
		"when": "vim.active && explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
	},
	{
		"key": "enter",
		"command": "list.select",
		"when": "vim.active && explorerViewletVisible && filesExplorerFocus"
	},
	{
		"key": "q",
		"command": "workbench.action.closeSidebar",
		"when": "vim.active && explorerViewletVisible && explorerViewletFocus && !editorFocus && !inputFocus && !textInputFocus && !renameInputVisible"
	},
	{
		"key": "v",
		"command": "explorer.openToSide",
		"when": "vim.active && explorerViewletVisible && filesExplorerFocus && !inputFocus"
	},
	{
		"key": "r",
		"command": "renameFile",
		"when": "vim.active && explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
	},
	{
		"key": "d",
		"command": "deleteFile",
		"when": "vim.active && explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !explorerResourceReadonly && !inputFocus"
	},
	{
		"key": "x",
		"command": "list.collapse",
		"when": "vim.active && explorerViewletVisible && filesExplorerFocus && !inputFocus"
	},
	{
		"key": "a",
		"command": "explorer.newFile",
		"when": "vim.active && explorerViewletVisible && filesExplorerFocus && !inputFocus"
	},
	{
		"key": "v",
		"command": "explorer.openToSide",
		"when": "vim.active && explorerViewletVisible && filesExplorerFocus && !inputFocus"
	},
	{
		"key": "y",
		"command": "filesExplorer.copy",
		"when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !inputFocus"
	},
	{
		"key": "ctrl+w",
		"command": "workbench.action.focusRightGroup",
		"when": "vim.active && explorerViewletVisible && !editorFocus && !inputFocus"
	},
	{
		"key": "space n",
		"command": "workbench.files.action.focusFilesExplorer",
		"when": "vim.active && !explorerViewletVisible && (!inputFocus || editorFocus) && vim.mode == 'Normal'"
	},
	{
		"key": "space n",
		"command": "workbench.action.closeSidebar",
		"when": "vim.active && explorerViewletVisible && (!inputFocus || editorFocus) && vim.mode == 'Normal'"
	},
	{
		"key": "space e",
		"command": "revealInExplorer",
		"when": "vim.active && !explorerViewletVisible && (!inputFocus || editorFocus)  && vim.mode == 'Normal'"
	},
	{
		"key": "space e",
		"command": "workbench.action.closeSidebar",
		"when": "vim.active && explorerViewletVisible && (!inputFocus || editorFocus) && vim.mode == 'Normal'"
	},
	{
		"key": "space t",
		"command": "workbench.action.toggleSidebarVisibility",
		"when": "vim.active && (!inputFocus || editorFocus) && vim.mode == 'Normal'"
	},
	{
		"key": "space t",
		"command": "workbench.action.toggleSidebarVisibility",
		"when": "vim.active && (!inputFocus || editorFocus) && vim.mode == 'Normal'"
	},
	{
		"key": "space f",
		"command": "workbench.action.quickOpen",
		"when": "vim.active && (!inputFocus || editorFocus) && vim.mode == 'Normal'"
	},
	{
		"key": "space j",
		"command": "workbench.action.findInFiles",
		"when": "vim.active && (!inputFocus || editorFocus) && vim.mode == 'Normal'"
	},
	// General and macros
	{
		"key": "shift+h",
		"command": "workbench.action.previousEditorInGroup",
		"when": "vim.active && vim.mode != 'Insert'"
	},
	{
		"key": "shift+l",
		"command": "workbench.action.nextEditorInGroup",
		"when": "vim.active && vim.mode != 'Insert'"
	},
	{
		"key": "ctrl+h",
		"command": "workbench.action.moveEditorLeftInGroup",
		"when": "vim.active && editorFocus"
	},
	{
		"key": "ctrl+l",
		"command": "workbench.action.moveEditorRightInGroup",
		"when": "vim.active && editorFocus"
	},
	{
		"key": "ctrl+p",
		"command": "workbench.action.pinEditor",
		"when": "vim.active && !activeEditorIsPinned && !inputFocus && vim.mode == 'Normal'"
	},
	{
		"key": "ctrl+p",
		"command": "workbench.action.unpinEditor",
		"when": "vim.active && activeEditorIsPinned && !inputFocus && vim.mode == 'Normal'"
	},
	{
		"key": "escape",
		"command": "workbench.action.navigateEditorGroups",
		"when": "vim.active && !editorFocus"
	},
	{
		"key": "escape",
		"command": "runCommands",
		"args": {
			"commands": [
				"search.action.clearSearchResults",
				"revealInExplorer",
				"workbench.action.closeSidebar"
			]
		},
		"when": "vim.active && inSearchEditor || searchViewletFocus || hasSearchResult"
	},
	{
		"key": "escape",
		"command": "runCommands",
		"args": {
			"commands": [
				"problems.action.clearFilterText",
				"workbench.actions.view.toggleProblems"
			]
		},
		"when": "vim.active && problemFocus || problemsFilterFocus"
	},
	{
		"key": "ctrl+n",
		"command": "list.focusDown",
		"when": "vim.active && listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "ctrl+p",
		"command": "list.focusUp",
		"when": "vim.active && listFocus || problemFocus || problemsFilterFocus"
	},
	{
		"key": "space a",
		"command": "editor.action.codeAction",
		"when": "vim.active && editorHasCodeActionsProvider && (!inputFocus || editorFocus) && vim.mode != 'Insert'"
	},
	{
		"key": "space s",
		"command": "codelens.showLensesInCurrentLine",
		"when": "vim.active && (!inputFocus || editorFocus) && vim.mode != 'Insert'"
	},
	{
		"key": "space r r",
		"command": "editor.action.refactor",
		"when": "vim.active && (!inputFocus || editorFocus) && editorHasCodeActionsProvider && vim.mode != 'Insert'"
	},
	{
		"key": "ctrl+p",
		"command": "-workbench.action.quickOpen",
		"when": "vim.active"
	},
	{
		"key": "ctrl+n",
		"command": "-workbench.action.files.newUntitledFile",
		"when": "vim.active"
	},
	// code actions
	{
		"key": "ctrl+n",
		"command": "selectNextCodeAction",
		"when": "vim.active && codeActionMenuVisible"
	},
	{
		"key": "ctrl+p",
		"command": "selectPrevCodeAction",
		"when": "vim.active && codeActionMenuVisible"
	},
	{
		"key": "ctrl+j",
		"command": "selectNextCodeAction",
		"when": "vim.active && codeActionMenuVisible"
	},
	{
		"key": "ctrl+k",
		"command": "selectPrevCodeAction",
		"when": "vim.active && codeActionMenuVisible"
	},
	// language commands
	{
		"key": "space d",
		"command": "go.show.commands",
		"when": "vim.active && (!inputFocus || editorFocus) && vim.mode != 'Insert' && editorLangId == 'go'"
	}
]
